{
	"name": "python-analysis",
	"displayName": "Python parsing and program analysis",
	"enableProposedApi": true,
	"version": "0.0.1",
	"scripts": {
		"build:parser": "node ./node_modules/jison/lib/cli.js --outfile src/python/python3.js src/python/python3.jison",
		"compile": "tsc -p ./",
		"test": "karma start src/test/karma.conf.js",
		"test:debug": "karma start src/test/karma.debug.conf.js",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/chai": "^4.1.4",
		"@types/mocha": "^5.2.4",
		"chai": "^4.2.0",
		"jison": "^0.4.18",
		"karma": "^4.0.0",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^3.0.5",
		"mocha": "^6.1.4",
		"ts-loader": "^6.0.1",
		"tslint": "^5.11.1",
		"typescript": "^3.2.4",
		"webpack": "^4.31.0",
		"webpack-cli": "^3.1.0"
	},
	"dependencies": {
		"@jupyterlab/services": "^3.2.1",
		"js-yaml": "^3.13.1"
	}
}
